{
  "name": "unmous/regalia_plugin",
  "type": "digraph-plugin",
  "require": {
    "digraphcms/digraph-core": "^2",
    "unmous/core_plugin": "^2",
    "ext-fileinfo": "*"
  },
  "require-dev": {
    "php": ">=8.1",
    "phpstan/phpstan": "^1.9",
    "robmorgan/phinx": "^0.13.4"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/UNM-OUS/core_plugin.git"
    }
  ],
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "DigraphCMS_Plugins\\unmous\\regalia\\": "src/"
    }
  },
  "scripts": {
    "migrate": [
      "phinx migrate"
    ],
    "seed": [
      "phinx seed:run"
    ]
  }
}